{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods":["GET","POST","PUT"],
      "headers":{
        "Access-Control-Allow-Origin" : "https://web-scrap-pi.vercel.app"
      }
    }
  ]
}